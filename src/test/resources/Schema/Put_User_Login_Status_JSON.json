  "type": "object",
  "properties": {
    "loginStatus": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "roleIds": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "status": {
      "type": "string"
    },
    "userLoginEmail": {
      "type": "string",
