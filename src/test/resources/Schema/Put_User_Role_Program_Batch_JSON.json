{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "programId": {
      "type": "integer"
    },
    "roleId": {
      "type": "string"
    },
    "userId": {
      "type": "string"
    },
    "userRoleProgramBatches": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "batchId": {
            "type": "integer"
          },
          "userRoleProgramBatchStatus": {
            "type": "string"
          }
        },
        "required": ["batchId", "userRoleProgramBatchStatus"]
      }
    }
  },
  "required": ["programId", "roleId", "userId", "userRoleProgramBatches"]
}